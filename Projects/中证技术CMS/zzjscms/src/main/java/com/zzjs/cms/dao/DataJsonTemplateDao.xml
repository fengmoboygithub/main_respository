			</if>
			<if test="updateUser != null and updateUser !=0 ">
				#{updateUser,jdbcType=BIGINT} ,
			</if>
		</trim>
	</sql>
	
	
	<!-- updateParams 更新列-->
	<sql id="updateParams">
		<trim suffix="" suffixOverrides=",">
			<if test="templateId != null and templateId !=0 ">
				templateId=#{templateId,jdbcType=BIGINT} ,
			</if>
			<if test="dataJsonEnName != null and dataJsonEnName !='' ">
				dataJsonEnName=#{dataJsonEnName,jdbcType=VARCHAR} ,
			</if>
			<if test="dataJsonName != null and dataJsonName !='' ">
				dataJsonName=#{dataJsonName,jdbcType=VARCHAR} ,
			</if>
			<if test="dataJsonCode != null and dataJsonCode !='' ">
