			</if>
		</trim>
	</sql>
	
	
	<insert id="addDataJsonTemplate" parameterType="com.zzjs.cms.entity.DataJsonTemplate" useGeneratedKeys="true" keyProperty="id">
		insert into 
		data_json_template_tab (
			<include refid="insertColumns" />
		) values (
			<include refid="insertParams" />
		)
		</insert>
	

	<!-- ############################## -->
	<!-- ###### 删除 ###### -->
	<!-- ############################## -->

	<update id="deleteByEntity" parameterType="com.zzjs.cms.entity.DataJsonTemplate">
		delete from data_json_template_tab 
		where 1=1
		<trim suffixOverrides=",">
			<if test="dataJsonEnName  != null and dataJsonEnName != ''">
	        	<![CDATA[ and  dataJsonEnName = #{dataJsonEnName } ]]>
			</if>
			<if test="id != null and id != 0">
	        	<![CDATA[ and  id != #{id}  ]]>
		    </if>
		    <if test="templateId  != null and templateId != 0">
	        	<![CDATA[ and  templateId = #{templateId } ]]>
			</if>
		</trim>
	</update>

	<!-- ############################## -->
	<!-- ###### 修改 ###### -->
	<!-- ############################## -->

	<update id="updateByPrimaryKey" parameterType="com.zzjs.cms.entity.DataJsonTemplate">
		update data_json_template_tab set 
		<if test="dataJsonEnName  != null and dataJsonEnName != ''">
			<![CDATA[ t.dataJsonEnName=#{dataJsonEnName}, ]]>
		</if>
		<if test="dataJsonName  != null and dataJsonName != ''">
			<![CDATA[ t.dataJsonName=#{dataJsonName}, ]]>
		</if>
		t.dataJsonCode=#{dataJsonCode}
		<include refid="Base_Where_Clause" />
	</update>
	
	<!-- 通过主键选择性更新 -->
	<update id="updateByIdSelective" parameterType="com.zzjs.cms.entity.DataJsonTemplate">
		update data_json_template_tab 
		<set>
			<include refid="updateParams" />
		</set>
		where id=#{id,jdbcType=BIGINT}
	</update>
	
	
	
	<!-- ############################## -->
