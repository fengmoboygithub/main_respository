			</if>
			<if test="templateId != null and templateId !=0 ">
				#{templateId,jdbcType=BIGINT} ,
			</if>
			<if test="dataJsonEnName != null and dataJsonEnName !='' ">
				#{dataJsonEnName,jdbcType=VARCHAR} ,
			</if>
			<if test="dataJsonName != null and dataJsonName !='' ">
				#{dataJsonName,jdbcType=VARCHAR} ,
			</if>
			<if test="dataJsonCode != null and dataJsonCode !='' ">
				#{dataJsonCode,jdbcType=VARCHAR} ,
			</if>
			<if test="isDelete != null and isDelete !='' ">
				#{isDelete,jdbcType=VARCHAR} ,
			</if>
			<if test="createTime != null   ">
				#{createTime,jdbcType=TIMESTAMP} ,
			</if>
			<if test="createUser != null and createUser !=0 ">
				#{createUser,jdbcType=BIGINT} ,
			</if>
			<if test="updateTime != null   ">
