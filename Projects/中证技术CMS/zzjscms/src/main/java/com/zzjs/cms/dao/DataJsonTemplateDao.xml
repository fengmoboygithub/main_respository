			#{templateId},
			#{dataJsonEnName},
			#{dataJsonName},
			#{dataJsonCode}
		)
		<selectKey resultType="long" keyProperty="id">
			SELECT
			LAST_INSERT_ID()
		</selectKey>
	</insert>
	
	<sql id="insertColumns">
		<trim suffix="" suffixOverrides=",">
			<if test="id != null and id !=0 ">
				id ,
			</if>
			<if test="templateId != null and templateId !=0 ">
				templateId ,
			</if>
			<if test="dataJsonEnName != null and dataJsonEnName !='' ">
				dataJsonEnName ,
			</if>
			<if test="dataJsonName != null and dataJsonName !='' ">
				dataJsonName ,
			</if>
			<if test="dataJsonCode != null and dataJsonCode !='' ">
				dataJsonCode ,
			</if>
			<if test="isDelete != null and isDelete !='' ">
				isDelete ,
			</if>
			<if test="createTime != null   ">
				createTime ,
			</if>
			<if test="createUser != null and createUser !=0 ">
				createUser ,
			</if>
			<if test="updateTime != null   ">
				updateTime ,
			</if>
			<if test="updateUser != null and updateUser !=0 ">
				updateUser ,
			</if>
		</trim>
	</sql>
	
	
	<!-- insertParams 入库值 -->
	<sql id="insertParams">
		<trim suffix="" suffixOverrides=",">
