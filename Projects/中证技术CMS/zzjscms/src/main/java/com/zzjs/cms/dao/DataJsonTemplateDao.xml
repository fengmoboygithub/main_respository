		<include refid="Base_Where_Clause" />
	</select>
	<select id="selectByModelPaging" resultMap="BaseResultMap"
		parameterType="com.zzjs.cms.entity.DataJsonTemplate">
		select
		<include refid="Base_Column_List" />
		from data_json_template_tab t
		<include refid="Base_Where_Clause" />
